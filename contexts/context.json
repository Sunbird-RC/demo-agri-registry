{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "id": "@id",
    "type": "@type",
    "schema": "https://schema.org/",
    "agri": "https://raw.githubusercontent.com/Sunbird-RC/demo-agri-registry/main/contexts/context.json",
    "FarmerCredential": {
      "@id": "agri:FarmerCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "farmerId": "schema:Text",
        "name": "schema:name",
        "address": {
          "@id": "schema:PostalAddress",
          "@context": {
            "@version": 1.1,
            "@protected": true,
            "addressLine1": "schema:streetAddress",
            "addressLine2": "schema:Text",
            "district": "schema:Text",
            "village": "schema:Text",
            "pincode": "schema:postalCode"
          }
        }
      }
    }
  }
}