{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "id": "@id",
    "type": "@type",
    "schema": "https://schema.org/",
    "farmer": "https://raw.githubusercontent.com/Sunbird-RC/demo-donor-registry/main/contexts/vc-context.json",
    "FarmerCredential": {
      "@id": "schema:farmer",
      "@context": {
        "id": "@id",
        "@version": 1.1,
        "@protected": true,
        "nonTransferable": "pledge:nonTransferable"
      }
    },
    "FarmerCredential": {
      "@id": "schema:Person",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "id": "schema:Text",
        "name": "schema:name",
        "fatherName": "schema:name",
        "bloodGroup": "schema:Text",
        "address": {
          "@id": "schema:PostalAddress",
          "@context": {
            "@version": 1.1,
            "@protected": true,
            "addressLine1": "schema:streetAddress",
            "addressLine2": "schema:Text",
            "district": "schema:Text",
            "village": "schema:Text",
            "pincode": "schema:postalCode"
          }
        }
      }
    }
  }
}